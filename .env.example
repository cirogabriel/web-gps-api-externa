# Variables de entorno de ejemplo
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_APP_NAME=GPS Tracker
VITE_APP_VERSION=1.0.0

# Configuración opcional para APIs de geolocalización externa
# Si no se proporcionan, se usarán las APIs gratuitas por defecto

# API de IPinfo (opcional - gratuita hasta 50,000 requests/mes)
# VITE_IPINFO_TOKEN=tu_token_aqui

# API de OpenCage (opcional - gratuita hasta 2,500 requests/día)
# VITE_OPENCAGE_API_KEY=tu_api_key_aqui

# Configuración de precisión
VITE_HIGH_ACCURACY_MODE=true
VITE_LOCATION_TIMEOUT=10000
VITE_UPDATE_INTERVAL=30000

# Firebase para tracking colaborativo (opcional)
# Crea un proyecto en https://console.firebase.google.com/
# VITE_FIREBASE_API_KEY=tu_firebase_api_key
# VITE_FIREBASE_AUTH_DOMAIN=tu-proyecto.firebaseapp.com
# VITE_FIREBASE_DATABASE_URL=https://tu-proyecto-default-rtdb.firebaseio.com/
# VITE_FIREBASE_PROJECT_ID=tu-proyecto
# VITE_FIREBASE_STORAGE_BUCKET=tu-proyecto.appspot.com
# VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
# VITE_FIREBASE_APP_ID=1:123456789:web:abcdef
